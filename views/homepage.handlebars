<!-- Loop over each object in the projects array -->
{{#each locations as |location| }}
//
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
     <!-- {{{get_emoji}}} -->
      <a href="/savedLocations/{{location.name}}">{{location.name}}</a>
    </h2>
    <p>
      
      <!-- Pass needed_funding value to the helper function -->
       <span class="risk-level">{{location.risk_level}}</span>
      needed!
    </p>
    <p>Quarantine needed:{{location.quarantine_needed}} on {{format_date location.date_added}}</p>
  </div>
  <div class="col-md-7">
    </p>
    <p>Created by {{location.user.id}} on {{format_date location.date_added}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{location.name}}
    </p>
  </div>
</div>
{{/each}}
